{
  "name": "@gadicc/valibot-serialize",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Convert Valibot schemas to/from a stable serialized AST",
  "tasks": {
    "dev": "deno run --watch main.ts",
    "build:npm": "deno run -A scripts/build_npm.ts",
    "test": "deno test --parallel",
    "coverage": "deno task test --coverage=coverage && deno coverage coverage --lcov --output=coverage.lcov",
    "pre-commit": "deno task lint && deno task test && deno task fmt",
    "lint": "deno lint",
    "fmt": "deno fmt",
    "tojson": "deno run tools/tojson.ts",
    "validate": "deno run tools/validate.ts",
    "gen-docs": "deno run --allow-write=docs tools/generate_mappings.ts",
    "playground": "deno run tools/playground.ts"
  },
  "imports": {
    "@biomejs/biome": "npm:@biomejs/biome@^2.2.4",
    "@biomejs/js-api": "npm:@biomejs/js-api@^3.0.0",
    "@biomejs/wasm-nodejs": "npm:@biomejs/wasm-nodejs@^2.2.4",
    "@deno/dnt": "jsr:@deno/dnt@^0.42.3",
    "@standard-schema/spec": "jsr:@standard-schema/spec@^1.0.0",
    "@std/assert": "jsr:@std/assert@1",
    "@std/cli": "jsr:@std/cli@^1.0.22",
    "@std/expect": "jsr:@std/expect@^1.0.17",
    "@std/fs": "jsr:@std/fs@^1.0.19",
    "@std/path": "jsr:@std/path@^1.1.2",
    "@std/testing": "jsr:@std/testing@^1.0.15",
    "drizzle-orm": "npm:drizzle-orm@^0.44.5",
    "drizzle-valibot": "npm:drizzle-valibot@^0.4.2",
    "drizzle-zod": "npm:drizzle-zod@^0.8.3",
    "fast-check": "npm:fast-check@^4.3.0",
    "prettier": "npm:prettier@^3.6.2",
    "semantic-release": "npm:semantic-release@^24.2.8",
    "@valibot/valibot": "npm:valibot@^1.1.0"
  },
  "exports": {
    ".": "./main.ts",
    "./tools/vs_tocode": "./tools/vs_tocode.ts"
  }
}
